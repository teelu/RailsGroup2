
<% title("Listing Employee Types") %>
<div class="body">
  <p id="notice"><%= notice %></p>
<table>
  <thead>
    <tr>
      <th>Type Code</th>
      <th>Type Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_types.each do |employee_type| %>
      <tr>
        <td><%= employee_type.typeCode %></td>
        <td><%= employee_type.typeName %></td>
        <td><%= link_to 'Show', employee_type %></td>
        <td><%= link_to 'Edit', edit_employee_type_path(employee_type) %></td>
        <td><%= link_to 'Destroy', employee_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Home', root_path %>

<%= link_to 'New Employee type', new_employee_type_path %>
</div>